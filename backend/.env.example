# Server Configuration
PORT=5000
NODE_ENV=development

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/fruitful_api

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Cloudflare Configuration (for production)
CLOUDFLARE_ACCOUNT_ID=
CLOUDFLARE_API_TOKEN=

# VaultLevel 7 Security
VAULT_ENCRYPTION_KEY=your-vault-encryption-key-32-chars
VAULT_LEVEL=7

# FAA-X13 Compliance
FAA_X13_COMPLIANCE_MODE=strict
FAA_X13_AUDIT_LOG=enabled

# AI API Keys
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
